{
    "name": "thebytelab/vapor-multi-region-deploy",
    "description": "An artisan command to assist deploying your Laravel Vapor app to multiple AWS regions",
    "keywords": [
        "laravel",
        "vapor"
    ],
    "authors": [
        {
            "name": "Josh Stevenson-Woods",
            "email": "josh@bytelab.solutions"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "symfony/yaml": "^4.2|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "autoload": {
        "psr-4": {
            "TheByteLab\\VaporMultiRegionDeploy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheByteLab\\VaporMultiRegionDeploy\\ServiceProvider"
            ]
        }
    }
}
